<h1>Posts</h1>


  <% @articles.each do |article| %>
    <div class="col">
      <div class="col-md-3">
        <div class="comment_head">
          <p><%= link_to article.title, article %></p>
        </div>
        <br>
      </div>
    </div>
  <% end %>

<button><%= link_to "New Post", new_article_path %></button>
